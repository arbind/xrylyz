$rylyzPlayerHost: '<%= RYLYZ_PLAYER_HOST %>';
$appName:	'wyjyt';
$assetsURL: 'http://' +$rylyzPlayerHost+ '/assets/';
$imagesURL: $assetsURL;
$appImagesURL: $imagesURL + 'images/' +$appName+ '/';


.wyjyt-app {
	color: #222;
  background-color: #fafafa;
  font-family: 'Helvetica Neue', helvetica, arial, sans-serif;
  font-size: 16px;
  margin: 0px auto;
  width: 300px;
  height: 200px;
	padding: 10px 8px;
  .space-screen {

  	.launch {
  		color: #2222FF;
  		position: relative;
  		width: 180px;
  		height: 180px;
  		clear: both;
  		margin: 20px auto;
  		background-color: #2C2D2B;
  		color: white;
	    border: 3px solid #101010;
	    &:hover {
	  		color: #CCCC88;
		    border-color: #CCCC88;
		    background-color: #3C3D3B;
	    }
		  border-radius: 8px;
			-o-border-radius: 8px;
			-moz-border-radius: 8px;
			-webkit-border-radius: 8px;
			.name {
				position: absolute;
				bottom: 0;
				width: 100%;
				text-align: center;
				line-height: 2em;
			}
  	}
  }

}
